{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module",
    "parser": "babel-eslint"
  },
  "plugins": ["babel"],
  "rules": {
    "prettier/prettier": "error",
    "import/prefer-default-export": "off",
    "quotes": ["error", "prefer-single"]
  }
}
